<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox fx:id="mainFrame" prefHeight="600.0" fx:controller="com.philippejung.controller.MainFrameController" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar>
        <menus>
          <Menu text="_File">
            <items>
              <MenuItem fx:id="menuFileExit" onAction="#onMenuFileExit" text="E_xit" />
            </items>
          </Menu>
          <Menu text="_Help">
            <items>
              <MenuItem fx:id="menuHelpAbout" onAction="#onMenuHelpAbout" text="A_bout" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ToolBar VBox.vgrow="NEVER">
        <items>
          <Button mnemonicParsing="false" text="Welcome" />
            <Button mnemonicParsing="false" text="Accounts"/>
            <Button mnemonicParsing="false" text="Import"  onAction="#onToolbarImport"/>
            <Button mnemonicParsing="false" text="Budget" onAction="#onToolbarBudget" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ToolBar>
      <TabPane fx:id="tabPane" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS">
        <tabs>
        </tabs>
      </TabPane>
      <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
         <children>
            <Pane HBox.hgrow="ALWAYS" />
            <Label text="Label" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
      </HBox>
   </children>
</VBox>
