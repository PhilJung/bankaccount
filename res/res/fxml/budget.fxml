<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<VBox spacing="5.0" stylesheets="@/res/css/budget.css" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.philippejung.bankaccount.controller.BudgetController">
    <HBox VBox.vgrow="NEVER">
        <Button mnemonicParsing="false" text="Button" HBox.hgrow="NEVER"/>
