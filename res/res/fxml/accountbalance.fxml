<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="vbox"
      stylesheets="@/res/css/accountbalance.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.philippejung.bankaccount.controller.AccountBalanceController">
    <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
    </padding>
    <LineChart fx:id="chart" createSymbols="false" styleClass="accountbalance" title="Soldes" VBox.vgrow="ALWAYS">
      <xAxis>
          <CategoryAxis fx:id="categoryAxis" side="BOTTOM"/>
      </xAxis>
      <yAxis>
          <NumberAxis fx:id="numberAxis" side="LEFT"/>
      </yAxis>
    </LineChart>
</VBox>
